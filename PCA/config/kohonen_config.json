{
    "root_path": "PCA/",
    "input": {
        "data_path": "in/europe.csv",
        "delimiter": ",",
        "header": true
    },
    "output": {
        "results_path": "out",
        "log_path": "logs/execution.log"
    },
    "pca_parameters": {
        "method": "kohonen",
        "k": 3,
        "init_r": 3,
        "init_learning_rate": 0.1,
        "epochs_rate": 200,
        "similarity": "euclidean",
        "replace": false
    }
}